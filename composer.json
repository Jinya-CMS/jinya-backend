{
  "name": "jinya/backend",
  "homepage": "https://jinya.de",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-zip": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "laminas/laminas-crypt": "3.3.1",
    "laminas/laminas-db": "2.11.3",
    "laminas/laminas-eventmanager": "3.3.0",
    "laminas/laminas-hydrator": "4.0.1",
    "laminas/laminas-json": "3.1.2",
    "laminas/laminas-serializer": "2.9.1",
    "league/plates": "3.3.0",
    "monolog/monolog": "2.1.1",
    "php-di/php-di": "6.3.0",
    "phpmailer/phpmailer": "v6.1.7",
    "phpmyadmin/sql-parser": "5.4.1",
    "scssphp/scssphp": "1.2.1",
    "slim/psr7": "1.3.0",
    "slim/slim": "4.7.1",
    "tedivm/jshrink": "v1.3.3",
    "vlucas/phpdotenv": "v5.2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "3.5.8"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public",
    "test": "./vendor/bin/phpunit",
    "phpcs": "phpcs --standard=PSR12"
  }
}
